# Cerberus Guard Bot Configuration
# Copy this file to .env and fill in your values

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Maximum guardian count before shutdown (default: 27)
MAX_GUARDIANS=27

# Initial guardians to spawn (default: 3)
INITIAL_GUARDIANS=3

# Guardian growth factor on bypass attempt (default: 3)
GUARDIAN_GROWTH_FACTOR=3
